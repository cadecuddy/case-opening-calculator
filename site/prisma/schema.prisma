// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

// datasource db {
//     provider = "sqlite"
//     url      = env("DATABASE_URL")
// }

model CaseListing {
    name        String   @id
    price       Float
    listings    Int
    image       String
    url         String
    lastUpdated DateTime
}
